case `grep '^SYSTEM=' ../../../config/Makefile` in
  SYSTEM=bsd_elf) exit 3;;
esac

case `sed -n -e '/OTHERLIBRARIES=/s// /p' ../../../config/Makefile` in
  *' unix'*) exit 0;;
  *) exit 3;;
esac
